   "source": [
    "con = sqlite3.connect('final.sqlite')\n",
    "final = pd.read_sql_query('''select * from reviews LIMIT 100000''',con)\n",
    "\n",
    "print(final.shape)\n",
    "final.head()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "metadata": {},
   "outputs": [],
   "source": [
    "preprocessed_data = final[\"CleanedText\"]\n",
    "scores = final['Score']\n",
    "x_tr, x_test, y_tr, y_test = train_test_split(preprocessed_data, scores, test_size = 0.3)\n",
    "from tqdm import tqdm\n",
    "def hyperparameter(x):\n",
    "    cv_scores = []\n",
    "    alpha = list(np.arange(0.01, 1, 0.01))\n",
    "    for a in tqdm(alpha):\n",
    "        naive_bayes = MultinomialNB(alpha = a)\n",
    "        score = cross_val_score(naive_bayes, x, y_tr, cv = 10, scoring = 'roc_auc')\n",
    "        cv_scores.append(score.mean())\n",
    "    \n",
    "    optimal_alpha = alpha[cv_scores.index(max(cv_scores))]\n",
    "    \n",
    "    sns.set_style("whitegrid")\n",
    "    plt.plot(alpha,cv_scores)\n",
    "    plt.xlabel('values of alpha')\n",
    "    plt.ylabel('area under ROC curve')\n",
    "    plt.show()\n",
    "    \n",
    "    return optimal_alpha\n"
   ]
  },
