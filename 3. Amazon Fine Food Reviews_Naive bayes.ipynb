   "source": [
    "## [1] Naive Bayes on Bag of Words vectors"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.preprocessing import StandardScaler\n",
    "Bow = CountVectorizer(ngram_range = (1,2), min_df = 10, max_features = 5000)\n",
    "train_bow = Bow.fit_transform(x_tr)\n",
    "test_bow = Bow.transform(x_test)\n",
    "\n",
    "std = StandardScaler(with_mean = False)\n",
    "train_bow = std.fit_transform(train_bow)\n",
    "test_bow = std.transform(test_bow)"
   ]
